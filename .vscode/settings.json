{
  "dotnet-test-explorer.addProblems": true,
  "dotnet-test-explorer.testProjectPath": "**/*Tests.@(csproj|vbproj|fsproj)",
  "dotnet.defaultSolution": "QuantLab.sln",
  "python-envs.defaultEnvManager": "ms-python.python:conda",
  "python-envs.defaultPackageManager": "ms-python.python:conda",
  "python-envs.pythonProjects": [
    {
      "path": "",
      "envManager": "ms-python.python:conda",
      "packageManager": "ms-python.python:conda"
    }
  ],
  "python.pythonPath": "src/strategies/.venv/Scripts/python.exe",
  "python.testing.pytestEnabled": true,
  "python.testing.pytestArgs": ["tests/strategies"],
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.formatting.provider": "black",
  "python.envFile": "${workspaceFolder}/.env"
}

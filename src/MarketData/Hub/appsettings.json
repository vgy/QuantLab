{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FilesStorage": {
    "Directory": "F:\\Projects\\GitHub\\Data",
    "SymbolsFileName": "symbols.csv",
    "SymbolsAndContractIdsFileName": "symbols_contractIds.csv",
    "RetrySymbolsAndContractIdsFileName": "retry_symbols_contractIds.csv",
    "HistoricalBarsRelativePathTemplate": "{0}\\{0}-{1}.csv"
  },
  "IbkrApi": {
    "BaseUrl": "https://localhost:5000",
    "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64)",
